{
  "seed": 42,
  "model": {
    "pretrained": true,
    "num_classes": 1000
  },
  "dataset": {
    "data_dir": "/sdc1/datasets/imagenet",
    "batch_size": 256,
    "resize_size": 256,
    "crop_size": 224,
    "num_workers": 8,
    "pin_memory": true
  },
  "optimizer": {
    "lr": 0.1,
    "momentum": 0.9,
    "weight_decay": 1e-4
  },
  "training": {
    "warmup_epochs": 5,
    "max_epochs": 300,
    "print_interval": 100,
    "save_dir": "./checkpoints",
    "distributed": false
  },
  "wandb": {
    "project": "resnet50",
    "name": "resnet50-MS"
  }
}
